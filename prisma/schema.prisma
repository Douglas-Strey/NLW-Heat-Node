datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String @id @default(uuid()) // primary key e auto_increment
  name       String
  github_id  Int
  avatar_url String
  login      String

  // Nome da tabela
  @@map("users")
}
